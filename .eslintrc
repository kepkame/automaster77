{
  "env": {
    "browser": true,
    "jquery": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "max-len": ["error", 120, 2, { "ignoreUrls": true }],
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": ["gulpfile.babel.js", "gulp/**/*"]}]
  }
}
