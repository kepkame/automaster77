mixin route(className)
  button(class= (className) ? `${className} route` : 'route', data-href="https://youtu.be/yDT3pU4p5Ms", aria-label= "Смотреть видеомаршрут проезда", title= "Видеомаршрут")
    +imgLazy('video-marshrut.jpg', 'Дорога улицы Беломорская в Москве')(class="route__img", width="235", height="131")
